package br.com.pet.test;

import java.sql.SQLException;

import br.com.pet.DAO.FornecedoresDAO;
import br.com.pet.domain.Fornecedores;
import br.com.pet.domain.Produtos;

public class ProdutoDAOTest {
	Produtos p1 = new Produtos();
	p1.setDescricao("Strodal");
	p1.setQuantidade(10);
	
	Fornecedores f = new Fornecedores();
	f.setCodigo(3)
	p1.setPreco(50,00);
	p1.setFornecedor(f);
	
	Produtos p2 = new Produtos();
	p2.setDescricao("Canex");
	
	FornecedoresDAO fao = new FornecedoresDAO();
	try {
		fao.save(f1);
		fao.save(f2);
		System.out.println("Salvo com Sucesso!!");
	} catch (SQLException e) {
		// TODO Auto-generated catch block
		e.printStackTrace();
		System.out.println("Ocorreu um Erro!!");
	}
}
